    },
    "parameters-check": {
      "Type": "Task",
      "Comment": "initial state of the flow",
      "Resource": "/state-conductor/actions/custom/parameters-check.sjs",
      "Parameters": {
        "name": ["David"]
      },
      "End": true
    }
  }
}