{
  "Comment": "is used in the executeStateByJobDoc",
  "mlDomain": {
    "context": [
      {
        "scope": "directory",
        "value": "/test/"
      },
      {
        "scope": "collection",
        "value": "test"
      }
    ]
  },
  "StartAt": "dialUp",
  "States": {
    "dialUp": {
      "Type": "Wait",
      "Event": "series-of-clicks-and-beeps-connected",
