{
  "Comment": "example of one flow that kicks off another",
  "mlDomain": {
    "context": [
      {
        "scope": "collection",
        "value": "chain"
      }
    ]
  },
  "StartAt": "add-collections",
  "States": {
    "add-collections": {
      "Type": "Task",
      "End": true,
      "Comment": "adds a collection which chaining-flow-B should pick up on",
      "Resource": "/state-conductor/actions/common/add-collections.sjs",
      "Parameters": {
        "collections": ["chain2"]
      }
    }
  }
}