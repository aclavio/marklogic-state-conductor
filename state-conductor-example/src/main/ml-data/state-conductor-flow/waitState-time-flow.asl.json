{
  "Comment": "wraps things in an envelope, and waits for an event",
  "mlDomain": {
    "context": [
      {
        "scope": "collection",
        "value": "waitStateTest"
      }
    ]
  },
  "StartAt": "wait_two_minute",
  "States": {
    "wait_two_minute": {
      "Type": "Wait",
      "Comment": "wait for 120 second to call the next state",
      "Seconds": 60,
      "Next": "needs-envelope"
    },
    "needs-envelope": {
      "Type": "Task",
      "Comment": "makes the envelope",
      "Resource": "/state-conductor/actions/custom/make-envelope.sjs",
      "Next": "make-headers"
    },
    "make-headers": {
      "Type": "Task",
      "Comment": "makes the envelope's triples",
      "Resource": "/state-conductor/actions/custom/make-headers",
      "Next": "success"
    },
    "success": {
      "Type": "Succeed"
    }
  }
}
