{
  "Comment": "Calls built-in to run a DHF5 flow step",
  "mlDomain": {
    "context": []
  },
  "StartAt": "runStep1",
  "States": {
    "runStep1": {
      "Type": "Task",
      "Comment": "runs the dhf 5 flow step",
      "Resource": "/state-conductor/actions/common/dhf/dhf5RunFlowStepAction.sjs",
      "Parameters": {
        "flowName": "MissingFlow",
        "step": 1,
        "flowOptions": {}
      },
      "Next": "Success"
    },
    "runStep99": {
      "Type": "Task",
      "Comment": "runs the dhf 5 flow step",
      "Resource": "/state-conductor/actions/common/dhf/dhf5RunFlowStepAction.sjs",
      "Parameters": {
        "flowName": "PersonFlow",
        "step": 99,
        "flowOptions": {}
      },
      "Next": "Success"
    },
    "Success": {
      "Type": "Succeed"
    }
  }
}
