{
  "extends": "eslint:recommended",
  "env": {
    "commonjs": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": {
    "eqeqeq": "warn",
    "indent": ["error", 2],
    "quotes": ["warn", "single"],
    "semi": ["error", "always"]
  },
  "globals": {
    "cts": false,
    "fn": false,
    "sem": false,
    "xdmp": false,
    "xs": false,
    "Sequence": false,
    "Symbol": false,
    "NodeBuilder": false,
    "Node": false,
    "XMLNode": false,
    "XMLDocument": false,
    "Document": false,
    "declareUpdate": false
  }
}
